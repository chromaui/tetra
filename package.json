{
  "name": "@chromaui/tetra",
  "version": "1.0.0",
  "description": "Style Guide + UI library for Chromatic and Storybook marketing sites and docs",
  "author": {
    "name": "cdedreuille"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chromaui/tetra.git"
  },
  "scripts": {
    "build": "tsup --onSuccess \"yarn yalc publish\"",
    "type-check": "tsc",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "lint:fix": "yarn lint --fix && prettier --write .",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "deploy-storybook": "storybook-to-ghpages",
    "release": "yarn build && yarn release-it",
    "chromatic": "npx chromatic --project-token=b80523143ab2"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "treeshake": true,
    "sourcemap": true,
    "minify": true,
    "clean": true,
    "dts": true,
    "splitting": false,
    "format": [
      "cjs",
      "esm"
    ],
    "external": [
      "react"
    ]
  },
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "eslint --ignore-path .gitignore --fix"
    ],
    "*": "prettier --write"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.0.2",
    "@storybook/addon-interactions": "^7.0.2",
    "@storybook/addon-links": "^7.0.2",
    "@storybook/blocks": "^7.0.2",
    "@storybook/react": "^7.0.2",
    "@storybook/react-vite": "^7.0.2",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@types/node": "18.15.3",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@typescript-eslint/eslint-plugin": "5.55.0",
    "@typescript-eslint/parser": "5.55.0",
    "@vitejs/plugin-react": "3.1.0",
    "babel-loader": "9.1.2",
    "chromatic": "6.17.3",
    "eslint": "8.36.0",
    "eslint-plugin-storybook": "0.6.11",
    "lint-staged": "13.2.0",
    "prettier": "2.8.4",
    "prop-types": "^15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "release-it": "15.9.0",
    "storybook": "7.0.2",
    "tsup": "6.6.3",
    "typescript": "5.0.2",
    "vite": "4.2.1",
    "yalc": "1.0.0-pre.53"
  },
  "peerDependencies": {
    "react": ">=17",
    "react-dom": ">=17"
  },
  "dependencies": {
    "@storybook/design-system": "7.15.8",
    "@storybook/theming": "7.0.2"
  },
  "bugs": {
    "url": "https://github.com/chromaui/tetra/issues"
  },
  "readme": "ERROR: No README data found!",
  "homepage": "https://github.com/chromaui/tetra#readme",
  "_id": "@chromaui/tetra@1.0.0"
}
